: banner   " nopforth" println ;

anon:
   init-load-paths
   #args if
      1 arg-offset +!  ['] file-abort to 'warm
      0 arg included  " can't run file" ?abort
      exit
   then
   banner  ['] term-loop to 'warm  term-loop
; execute
